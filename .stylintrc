{
  "blocks":false,
  "brackets":"always",
  "colons":"always",
  "colors":"always",
  "commaSpace":"always",
  "commentSpace":"always",
  "cssLiteral":"never",
  "customProperties":[],
  "depthLimit":false,
  "duplicates":true,
  "efficient":"always",
  "exclude":[],
  "extendPref":false,
  "globalDupe":true,
  "groupOutputByFile":true,
  "indentPref":2,
  "leadingZero":"never",
  "maxErrors":false,
  "maxWarnings":false,
  "mixed":false,
  "mixins":[],
  "namingConvention":"BEM",
  "namingConventionStrict":true,
  "none":"never",
  "noImportant":true,
  "parenSpace":false,
  "placeholders":"always",
  "prefixVarsWithDollar":"always",
  "quotePref":"double",
  "reporterOptions":{
    "columns":[
      "lineData",
      "severity",
      "description",
      "rule"
    ],
    "columnSplitter":"  ",
    "showHeaders":false,
    "truncate":true
  },
  "semicolons":"always",
  "sortOrder":"alphabetical",
  "stackedProperties":"never",
  "trailingWhitespace":"never",
  "universal":false,
  "valid":true,
  "zeroUnits":"never",
  "zIndexNormalize":false
}