{
  "name": "red-queen-boilerplate",
  "version": "0.1.0",
  "description": "A lightweight boilerplate to use with React-Redux projects",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "set NODE_ENV=development node && node_modules/.bin/webpack-dev-server --config ./config/dev.config.js",
    "build": "set NODE_ENV=production node && node_modules/.bin/webpack --config ./config/prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raffaeldantass/red-queen-boilerplate.git"
  },
  "keywords": [
    "React",
    "Redux",
    "Stylus",
    "Boilerplate"
  ],
  "author": "Rafael Dantas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raffaeldantass/red-queen-boilerplate/issues"
  },
  "homepage": "https://github.com/raffaeldantass/red-queen-boilerplate#readme",
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.12.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
